pipeline {
  agent any
    
  // tools {nodejs "node"}
    
  stages {
         
    stage('Sonar') {
      steps {
          echo 'executing yarn.....'
          bat 'npm install'
        bat 'set CI=true&&npm test'
      nodejs('node-14.14') 
      {
        bat 'yarn sonar'
        }
      }
    }  
    
    
  }
}
